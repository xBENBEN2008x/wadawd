	-- Favorites persistence
	local function split(str, sep)
		if not str or str == "" then return {} end
		sep = sep or ";"
		local t = {}
		for s in string.gmatch(str, "([^" .. sep .. "]+)") do
			table.insert(t, s)
		end
		return t
	end
	local function join(tbl, sep)
		sep = sep or ";"
		if not tbl or #tbl == 0 then return "" end
		return table.concat(tbl, sep)
	end

	local Favs = {}
	local stored = player:GetAttribute("NexorFavs")
	if stored and type(stored) == "string" and stored ~= "" then
		Favs = split(stored, ";")
	end
	local function saveFavs()
		player:SetAttribute("NexorFavs", join(Favs, ";"))
	end
	local function isFav(name) return table.find(Favs, name) ~= nil end

	-- script container & templates
	local scriptContainer = v48
	local template = v50
	local template2 = v65
	if template then template.Visible = false end
	if template2 then template2.Visible = false end

	-- helper to find child recursively
	local function findChildRecursive(parent, name)
		if not parent then return nil end
		for _, c in pairs(parent:GetChildren()) do
			if c.Name == name then return c end
			local found = findChildRecursive(c, name)
			if found then return found end
		end
		return nil
	end

	-- forward declare rebuildScriptList so createEntry can call it
	local rebuildScriptList

	local function findConfigByName(name)
		for _, d in ipairs(Config) do
			if d.Name == name then return d end
		end
		return nil
	end

	-- create entry
	local function createEntry(data, opts)
		opts = opts or {}
		local isPlaceMatch = opts.placeMatch or false
		if not template then return nil end
		local clone = template:Clone()
		clone.Name = "Entry_" .. tostring(data.Name)
		clone.Parent = scriptContainer
		clone.Visible = true

		local nameLabel = findChildRecursive(clone, "GameName")
		if nameLabel and nameLabel:IsA("TextLabel") then
			nameLabel.Text = tostring(data.Name)
		end

		-- background color logic
		if isPlaceMatch then
			if clone:IsA("Frame") then clone.BackgroundColor3 = Color3.fromRGB(0,120,255) end
		elseif isFav(data.Name) then
			if clone:IsA("Frame") then clone.BackgroundColor3 = Color3.fromRGB(255,204,0) end
		else
			if clone:IsA("Frame") then clone.BackgroundColor3 = Color3.fromRGB(180,5,255) end
		end

		-- Run
		local runBtn = findChildRecursive(clone, "Run")
		if runBtn and runBtn:IsA("TextButton") then
			runBtn.MouseButton1Click:Connect(function()
				local ok, err = pcall(function() data.Run() end)
				if not ok then warn("Fehler beim Run von '"..tostring(data.Name).."': "..tostring(err)) end
			end)
		end

		-- Info
		local infoBtn = findChildRecursive(clone, "Info")
		if infoBtn and infoBtn:IsA("TextButton") then
			infoBtn.MouseButton1Click:Connect(function()
				print("Info:", data.Name, "PlaceId:", tostring(data.PlaceId))
			end)
		end

		-- Fav
		local favBtn = findChildRecursive(clone, "fav")
		if favBtn and favBtn:IsA("TextButton") then
			local function updateFavVisual()
				if isFav(data.Name) then favBtn.Text = "‚òÖ" else favBtn.Text = "‚òÜ" end
				if isPlaceMatch then
					-- leave blue
				else
					if isFav(data.Name) then
						clone.BackgroundColor3 = Color3.fromRGB(255,204,0)
					else
						clone.BackgroundColor3 = Color3.fromRGB(180,5,255)
					end
				end
			end
			updateFavVisual()
			favBtn.MouseButton1Click:Connect(function()
				if isFav(data.Name) then
					table.remove(Favs, table.find(Favs, data.Name))
				else
					table.insert(Favs, data.Name)
				end
				saveFavs()
				rebuildScriptList(v47 and v47:IsA("TextBox") and v47.Text or "")
			end)
		end

		return clone
	end

	function rebuildScriptList(filter)
		filter = filter or ""
		-- clear existing (except templates)
		for _, f in ipairs(scriptContainer:GetChildren()) do
			if f:IsA("Frame") and (f ~= template and f ~= template2) then
				f:Destroy()
			end
		end

		local currentPlace = tostring(game.PlaceId)
		local usedNames = {}

		local placeMatches = {}
		local favMatches = {}
		local others = {}

		-- place matches
		for _, d in ipairs(Config) do
			if d.PlaceId and tostring(d.PlaceId) == currentPlace then
				if filter == "" or string.find(string.lower(d.Name), string.lower(filter)) then
					table.insert(placeMatches, d)
					usedNames[d.Name] = true
				end
			end
		end

		-- favorites in stored order
		for _, fname in ipairs(Favs) do
			if not usedNames[fname] then
				local d = findConfigByName(fname)
				if d then
					if filter == "" or string.find(string.lower(d.Name), string.lower(filter)) then
						table.insert(favMatches, d)
						usedNames[d.Name] = true
					end
				end
			end
		end

		-- others
		for _, d in ipairs(Config) do
			if not usedNames[d.Name] then
				if filter == "" or string.find(string.lower(d.Name), string.lower(filter)) then
					table.insert(others, d)
					usedNames[d.Name] = true
				end
			end
		end

		-- create entries in order: placeMatches, favMatches, others
		for _, d in ipairs(placeMatches) do
			createEntry(d, { placeMatch = true })
		end
		for _, d in ipairs(favMatches) do
			createEntry(d, { placeMatch = false })
		end
		for _, d in ipairs(others) do
			createEntry(d, { placeMatch = false })
		end

		-- adjust CanvasSize
		task.spawn(function()
			local count = 0
			for _,c in pairs(scriptContainer:GetChildren()) do
				if c:IsA("Frame") and c.Visible and c ~= template and c ~= template2 then count = count + 1 end
			end
			scriptContainer.CanvasSize = UDim2.new(0,0,0, math.max(1, count) * 115)
		end)
	end

	-- Search box live filter
	if v47 and v47:IsA("TextBox") then
		v47:GetPropertyChangedSignal("Text"):Connect(function()
			local txt = v47.Text or ""
			rebuildScriptList(txt)
		end)
	end

	-- initial build
	rebuildScriptList("")

	-- Run Now button behavior
	if v34 and v34:IsA("TextButton") then
		v34.MouseButton1Click:Connect(function()
			local currentPlace = tostring(game.PlaceId)
			local chosen = nil
			for _,d in ipairs(Config) do
				if tostring(d.PlaceId) == currentPlace then
					chosen = d
					break
				end
			end
			if not chosen then chosen = Config[1] end
			if chosen and chosen.Run then
				local ok, err = pcall(function() chosen.Run() end)
				if not ok then warn("Fehler in Run-Funktion: "..tostring(err)) end
			end
		end)
	end

	-- THEME handling
	local ThemeColors = {
		Default = {
			main = Color3.fromRGB(179, 1, 170),
			stroke = Color3.fromRGB(140, 1, 133),
			button = Color3.fromRGB(183,1,255),
			buttonStroke = Color3.fromRGB(140,1,133),
		},
		Black = {
			main = Color3.fromRGB(30,30,30),
			stroke = Color3.fromRGB(10,10,10),
			button = Color3.fromRGB(40,40,40),
			buttonStroke = Color3.fromRGB(0,0,0),
		},
		Blue = {
			main = Color3.fromRGB(12,97,255),
			stroke = Color3.fromRGB(0,80,200),
			button = Color3.fromRGB(0,120,255),
			buttonStroke = Color3.fromRGB(0,80,200),
		},
		Green = {
			main = Color3.fromRGB(24,200,50),
			stroke = Color3.fromRGB(20,170,40),
			button = Color3.fromRGB(47,255,0),
			buttonStroke = Color3.fromRGB(20,170,40),
		},
	}

	local currentTheme = player:GetAttribute("NexorTheme") or "Default"
	if not ThemeColors[currentTheme] then currentTheme = "Default" end

	local function applyTheme(themeName)
		local t = ThemeColors[themeName]
		if not t then return end
		currentTheme = themeName
		player:SetAttribute("NexorTheme", themeName)
		-- MainFrame
		if v2 and v2:IsA("Frame") then pcall(function() v2.BackgroundColor3 = t.main end) end
		if v4 and v4:IsA("UIStroke") then pcall(function() v4.Color = t.stroke end) end
		-- Sidebar buttons (first-level children)
		for _, btn in pairs(v7:GetChildren()) do
			if btn:IsA("TextButton") then
				pcall(function() btn.BackgroundColor3 = t.button end)
				for _, c in pairs(btn:GetChildren()) do
					if c:IsA("UIStroke") then
						pcall(function() c.Color = t.buttonStroke end)
					end
				end
			end
		end
		-- Misc background frame (v85)
		if v85 and v85:IsA("Frame") then pcall(function() v85.BackgroundColor3 = t.button end) end
	end

	-- wire theme buttons
	if v87 and v87:IsA("TextButton") then v87.MouseButton1Click:Connect(function() applyTheme("Default") end) end
	if v88 and v88:IsA("TextButton") then v88.MouseButton1Click:Connect(function() applyTheme("Blue") end) end
	if v89 and v89:IsA("TextButton") then v89.MouseButton1Click:Connect(function() applyTheme("Green") end) end
	if v90 and v90:IsA("TextButton") then v90.MouseButton1Click:Connect(function() applyTheme("Black") end) end
	applyTheme(currentTheme)

	-- Keybind (toggle GUI)
	local defaultKey = "RightShift"
	local function parseKeyName(name)
		if not name or name == "" then return Enum.KeyCode.RightShift end
		if Enum.KeyCode[name] then return Enum.KeyCode[name] end
		-- try uppercase
		local up = string.upper(name)
		if Enum.KeyCode[up] then return Enum.KeyCode[up] end
		return Enum.KeyCode.RightShift
	end

	local savedBind = player:GetAttribute("NexorKeyBind")
	if savedBind == nil then
		player:SetAttribute("NexorKeyBind", defaultKey)
		savedBind = defaultKey
	end

	if v93 and v93:IsA("TextLabel") then v93.Text = "["..tostring(savedBind).."]" end
	local currentBindEnum = parseKeyName(savedBind)

	local function toggleGuiVisible()
		if v1 and v1:IsA("ScreenGui") and v2 and v2:IsA("Frame") then
			v2.Visible = not v2.Visible
		end
	end

	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
		if input.KeyCode == currentBindEnum then
			toggleGuiVisible()
		end
	end)

	-- set keybind via UI
	if v94 and v94:IsA("TextBox") and v95 and v95:IsA("TextButton") then
		v95.MouseButton1Click:Connect(function()
			local text = (v94.Text or ""):gsub("%s+","")
			if text == "" then return end
			if Enum.KeyCode[text] == nil then
				local up = string.upper(text)
				if Enum.KeyCode[up] then text = up end
			end
			if Enum.KeyCode[text] then
				player:SetAttribute("NexorKeyBind", text)
				currentBindEnum = Enum.KeyCode[text]
				if v93 and v93:IsA("TextLabel") then v93.Text = "["..tostring(text).."]" end
				v94.Text = ""
			else
				local prev = v94.PlaceholderText
				v94.PlaceholderText = "Ung√ºltiger Key (z.B. RightShift)"
				v94.Text = ""
				task.delay(1.5, function() v94.PlaceholderText = prev end)
			end
		end)
	end

	-- attribute change listeners
	player:GetAttributeChangedSignal("NexorFavs"):Connect(function()
		local s = player:GetAttribute("NexorFavs")
		if s and type(s) == "string" then
			Favs = split(s, ";")
		else
			Favs = {}
		end
		rebuildScriptList(v47 and v47:IsA("TextBox") and v47.Text or "")
	end)
	player:GetAttributeChangedSignal("NexorTheme"):Connect(function()
		local t = player:GetAttribute("NexorTheme")
		if t and ThemeColors[t] then applyTheme(t) end
	end)
	player:GetAttributeChangedSignal("NexorKeyBind"):Connect(function()
		local k = player:GetAttribute("NexorKeyBind")
		if k and Enum.KeyCode[k] then
			currentBindEnum = Enum.KeyCode[k]
			if v93 and v93:IsA("TextLabel") then v93.Text = "["..tostring(k).."]" end
		end
	end)

	-- final touches
	rebuildScriptList("")
	updateGameNameUI()

	print("[NexorGui] Full script loaded: Theme, Keybind, Favorites, PlaceId-Highlight active.")
end

if type(__converted) == 'function' then
	local success, err = pcall(__converted)
	if not success then warn('Error running converted module: '..tostring(err)) end
else
	warn('Converted module \'NexorGui_ExportedModule\' did not return a function')
end
