	local Players = game:GetService("Players")
	local MarketplaceService = game:GetService("MarketplaceService")
	local UserInputService = game:GetService("UserInputService")

	-- safety helper
	local function safe(name, obj)
		if not obj then
			warn("Erwartetes GUI-Objekt fehlt: "..tostring(name))
			return false
		end
		return true
	end

	if not (safe("MainFrame (v2)", v2) and safe("MainFrame UIStroke (v4)", v4) and safe("SideBarScrollingFrame (v7)", v7)
		and safe("ScriptContainer (v48)", v48) and safe("template (v50)", v50)) then
		return
	end

	-- ensure gui not removed on respawn
	pcall(function() v1.ResetOnSpawn = false end)
	player.CharacterAdded:Connect(function()
		task.delay(0.5, function()
			local pg = player:FindFirstChild("PlayerGui")
			if pg and v1 and v1.Parent ~= pg then
				pcall(function() v1.Parent = pg end)
			end
		end)
	end)

	-- update game name labels
	local function safeGetPlaceName()
		local ok, info = pcall(function()
			return MarketplaceService:GetProductInfo(game.PlaceId)
		end)
		if ok and type(info) == "table" and info.Name then
			return info.Name
		else
			return "Unknown"
		end
	end

	local function updateGameNameUI()
		local gamename = safeGetPlaceName()
		pcall(function() if v5 and v5:IsA("TextLabel") then v5.Text = "Nexor | " .. tostring(gamename) end end)
		pcall(function() if v30 and v30:IsA("TextLabel") then v30.Text = tostring(gamename) end end)
	end
	updateGameNameUI()

	-- frames map
	local frames = {
		Home = v24,
		Scripts = v44,
		Misc = v80,
		Profile = v99,
		News = v104
	}
	local function showFrame(name)
		for k, f in pairs(frames) do
			if f and f:IsA("Instance") then
				f.Visible = (k == name)
			end
		end
	end

	-- connect sidebar buttons
	local function safeConnectButton(btn, name)
		if btn and btn:IsA("TextButton") then
			btn.MouseButton1Click:Connect(function()
				showFrame(name)
			end)
		end
	end
	safeConnectButton(v9, "Home")
	safeConnectButton(v12, "Scripts")
	safeConnectButton(v15, "Misc")
	safeConnectButton(v18, "Profile")
	safeConnectButton(v21, "News")
	showFrame("Home")

	-- Config table (deine Eintr√§ge; unver√§ndert)
